#include <iostream>
#include<bits/stdc++.h>
#include<cstring>
#include<algorithm>

using namespace std;

int main()
{
    char s[11];
    int len,i;
    scanf("%s",s);
    len=strlen(s);

    sort(s,s+len);
     len--;
    i=len;
    cout<<s[len];
    while(--i>=0&&s[i]==s[len])
    {
        cout<<s[len];
    }
    cout<<endl;
    return 0;
}
